{
  "name": "vite-plugin-chunk-split",
  "version": "0.5.2",
  "description": "A vite plugin for better chunk split",
  "author": "sbauschl",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": {
    "url": "git+ssh://git@github.com/sbauschl/vite-plugin-chunk-split.git"
